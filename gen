    <script id="GENERAT-URL">    <!--script randomURL
        function gass(a) {
            var url1Array = a.split('\n');
            for (var i = 0; i < url1Array.length; i++) {
                var domain = Math.floor(Math.random() * url1Array.length);
                document.getElementById("genme").value = "http://" + url1Array[domain] + "/?KOLOR=" + awur();
            }
        }
        function awur() {
            var text = "";
            var possible = "1234567890abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";
            for (var i = 0; i < 10; i++)text += possible.charAt(Math.floor(Math.random() * possible.length));
            return text
        }
    </script>
	
    <script id="GENERAT-URL2">    <!--script randomURL2
        function gass2(a) {
            var url1Array = a.split('\n');
            for (var i = 0; i < url1Array.length; i++) {
                var domain = Math.floor(Math.random() * url1Array.length);
                document.getElementById("genme2").value = "http://" + url1Array[domain] + "/?KOLOR=" + awur();
            }
        }
        function awur() {
            var text = "";
            var possible = "1234567890abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";
            for (var i = 0; i < 10; i++)text += possible.charAt(Math.floor(Math.random() * possible.length));
            return text
        }
    </script>

    <script type="text/javascript">    <!--script copy
        (function () {
            'use strict';
            document.body.addEventListener('click', copy, true);
            function copy(e) {
                var t = e.target,
                    c = t.dataset.copytarget,
                    inp = (c ? document.querySelector(c) : null);
                if (inp && inp.select) {
                    inp.select();
                    try {
                        document.execCommand('copy');
                        inp.blur();
                        inp.focus();
                        t.classList.add('copied');
                        setTimeout(function () {
                            t.classList.remove('copied');
                        }, 1500);
                    } catch (err) {
                        swal.fire('please press Ctrl/Cmd+C to copy');
                    }
                }
            }
        })();
    </script>
	
